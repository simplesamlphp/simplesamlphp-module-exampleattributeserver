{
    "name": "simplesamlphp/simplesamlphp-module-exampleattributeserver",
    "description": "An example for SAML attributes queries",
    "type": "simplesamlphp-module",
    "keywords": ["simplesamlphp", "exampleattributeserver"],
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Olav Morken",
            "email": "olavmrk@gmail.com"
        }
    ],
    "config": {
        "preferred-install": {
            "simplesamlphp/simplesamlphp": "source",
            "*": "dist"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "simplesamlphp/composer-module-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "simplesamlphp/composer-xmlprovider-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "SimpleSAML\\Module\\exampleattributeserver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSAML\\Test\\Utils\\": "vendor/simplesamlphp/simplesamlphp/tests/Utils"
        }
    },
    "require": {
        "php": "^8.1",

        "nyholm/psr7": "~1.8.2",
        "simplesamlphp/assert": "~1.9.0",
        "simplesamlphp/saml2": "~5.0.3",
        "simplesamlphp/simplesamlphp": "~2.4.0",
        "simplesamlphp/xml-common": "~1.25.0",
        "simplesamlphp/xml-security": "~1.13.0",
        "symfony/http-foundation": "~6.4.0",
        "symfony/psr-http-message-bridge": "~6.4.0"
    },
    "require-dev": {
        "beste/clock": "~3.0.0",
        "mockery/mockery": "~1.6.12",
        "simplesamlphp/simplesamlphp-test-framework": "~1.9.3"
    },
    "support": {
        "issues": "https://github.com/simplesamlphp/simplesamlphp-module-exampleattributeserver/issues",
        "source": "https://github.com/simplesamlphp/simplesamlphp-module-exampleattributeserver"
    }
}
